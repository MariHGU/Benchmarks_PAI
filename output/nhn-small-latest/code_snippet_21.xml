<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
    <xsl:template match="@*|node()">
        <xsl:copy>
            <xsl:apply-templates select="@*|node()"/>
        </xsl:copy>
    </xsl:template>
    
    <!-- Ignore THIS-IS-PART-OF-DESCRIPTION tags -->
    <xsl:template match="THIS-IS-PART-OF-DESCRIPTION">
        <xsl:value-of select="."/>
    </xsl:template>
</xsl:stylesheet>